        %no_lookup
         intros N. unfold.
           %X != X1
            intros E. case E. apply no_lookup to N LTy.
           %no_lookup LRemain X1
            N': apply RAS1 to N.
            apply remove_all_no_lookup_back to RS1 N'. search.
        %rest
         search.
     %LS-Later
      N: apply RAS1 to LTy. apply no_lookup_mem to N RS.
  %RS-Later
   RAS: case RAS. LTy: case LTy.
     %LS-FirstScope
