      LB: case LB.
        %Lkp-Here
         search.
        %Lkp-Later
         apply LB to _.
     %Lkp-Later
      LB: case LB.
        %Lkp-Here
         apply LA to _.
        %Lkp-Later
         apply IH to PF1 LA1 LB1. search.


Theorem projedFields_replaceRecVal : forall FA FB F VA VB RA RB,
  projedFields FA FB -> projedVal VA VB ->
  replaceRecVal F VA FA RA -> replaceRecVal F VB FB RB ->
  projedFields RA RB.
induction on 3. intros PF PV RA RB. PF: case PF.
  %PF-Nil
   case RA.
  %PF-Cons
   RA: case RA.
     %RRV-Here
      RB: case RB.
        %RRV-Here
         search.
        %RRV-Later
         apply RB to _.
     %RRV-Later
      RB: case RB.
        %RRV-Here
         apply RA to _.
        %RRV-Later
         apply IH to _ _ RA1 RB1. search.


Theorem projedFields_updateRecFields : forall FA FB Fs VA VB RA RB,
  is_list (is_pair is_string is_value) FA ->
  is_list (is_pair is_string is_value) FB ->
  projedFields FA FB -> projedVal VA VB ->
  updateRecFields Fs VA FA RA -> updateRecFields Fs VB FB RB ->
  projedFields RA RB.
induction on 5. intros IsA IsB PF PV UA UB. PF: case PF.
  %PF-Nil
   UA: case UA.
     %URF-One
      case UA.
     %URF-Step
      case UA.
  %PF-Cons
   UA: case UA.
     %URF-One
      UB: case UB.
        %URF-One
         apply projedFields_replaceRecVal to _ _ UA UB. search.
        %URF-Step
         case UB2.
     %URF-Step
      UB: case UB.
        %URF-One
         case UA2.
        %URF-Step
         Pr: apply projedFields_lookup to _ UA UB.
         apply lookup_is_value to _ UA.
         PF': apply matchRec_projedVal to Pr _ UA1.
         case IsA. apply matchRec_is to _ UA1.
         apply lookup_is_value to _ UB.
         apply matchRec_unique to _ PF' UB1.
         apply matchRec_is to _ PF'. apply IH to _ _ _ _ UA2 UB2.
         apply projedFields_replaceRecVal to _ _ UA3 UB3. search.




/********************************************************************
 Projection Constraints for Evaluation
 ********************************************************************/
%projection evaluates to a related value
Projection_Constraint proj_evalExpr_forward :
  forall E E' FE EE V O,
    Pr : |{expr}- E ~~> E' ->
    IsE : is_expr E ->
    IsFE : is_list (is_pair is_string
                   (is_pair is_string
                   (is_pair is_value
                   (is_pair (is_list is_string) is_stmt)))) FE ->
    IsEE : is_list (is_list (is_pair is_string is_value)) EE ->
    Ev : evalExpr FE EE E V O ->
    exists V', evalExpr FE EE E' V' O /\
               projedVal V V'.


%projection evaluates to a related context
Projection_Constraint proj_evalStmt_forward :
  forall Names S S' FE EE EE' O,
    Pr : Names |{stmt}- S ~~> S' ->
    Names : names EE Names ->
    IsS : is_stmt S ->
    IsFE : is_list (is_pair is_string
                   (is_pair is_string
                   (is_pair is_value
                   (is_pair (is_list is_string) is_stmt)))) FE ->
    IsEE : is_list (is_list (is_pair is_string is_value)) EE ->
    Ev : evalStmt FE EE S EE' O ->
    exists EE'', evalStmt FE EE S' EE'' O /\
                 scopes_same EE' EE''.


/*
  We don't include any projection constraints for argument and record
  field expression evaluation because we introduced properties above
  to make the types nonextensible, so there cannot actually be
  projecting syntax for them.
*/




